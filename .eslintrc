{
  "extends": "airbnb",
  "parserOptions": {
      "ecmaVersion": 6
  },
  "env": {
      "node": true,
      "browser": true,
      "es6": true
  },
  "rules": {
      "linebreak-style": 0,
      "comma-dangle": [
          "error",
          "never"
      ],
      "react/jsx-filename-extension": 0
  }
}